<p id="notice"><%= notice %></p>

<h1>My Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Url</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_posts.each do |my_post| %>
      <tr>
        <td><%= my_post.title %></td>
        <td><%= my_post.content %></td>
        <td><%= my_post.url %></td>
        <td><%= my_post.time %></td>
        <td><%= link_to 'Show', my_post %></td>
        <td><%= link_to 'Edit', edit_my_post_path(my_post) %></td>
        <td><%= link_to 'Destroy', my_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My Post', new_my_post_path %>
